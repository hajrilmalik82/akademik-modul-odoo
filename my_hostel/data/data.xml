<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <record id="res_partner_packt" model="res.partner">
            <field name="name">Packt Publishing</field>
            <field name="city">Birmingham</field>
            <field name="country_id" ref="base.uk" />
        </record>

        <record id="cat_all_rooms" model="hostel.category">
            <field name="name">All rooms</field>
        </record>
        
        <function model="hostel.room" name="update_price_by_category" eval="(ref('my_hostel.cat_all_rooms'), 2)"/>
    </data>
</odoo>