<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="base.main_company" model="res.company">
            <field name="name">Packt publishing</field>
        </record>

        <record id="student_husen" model="hostel.student">
            <field name="name">Husen Daudi</field>
            <field name="gender">male</field>
            <field name="active">True</field>
        </record>

        <record id="student_jay" model="hostel.student">
            <field name="name">Jay Vora</field>
            <field name="gender">male</field>
            <field name="active">True</field>
        </record>

        <record id="room_demo_01" model="hostel.room">
            <field name="name">Hostel Room Demo</field>
            <field name="room_no">HR-DEMO-001</field>
            <field name="rent_amount">750000</field>
            <field name="student_per_room">4</field> 

            <field name="student_ids" eval="[
                (4, ref('student_husen')),
                (4, ref('student_jay'))
            ]"/>
        </record>
    </data>
</odoo>