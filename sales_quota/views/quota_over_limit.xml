<odoo>
    <record id="view_quota_over_limit_tree" model="ir.ui.view">
        <field name="name">quota.over.limit.tree</field>
        <field name="model">quota.over.limit</field>
        <field name="arch" type="xml">
            <tree string="Over Quota Records" create="false" edit="false">
                <field name="year"/>
                <field name="month"/>
                <field name="partner_id"/>
                <field name="quota_category_id"/>
                <field name="limit_qty"/>
                <field name="used_qty"/>
                <field name="over_qty" decoration-danger="1"/>
            </tree>
        </field>
    </record>

    <record id="action_quota_over_limit" model="ir.actions.act_window">
        <field name="name">Over Quota Records</field>
        <field name="res_model">quota.over.limit</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>